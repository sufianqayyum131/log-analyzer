<!--
this is used by Archiver HTTP interface for the print template. It filters malicious tags and also tags such as <html> which appear in valid html documents.
Only used by Rodion
-->
<config1>
	<version ver="9" />
	<whitelist list="&lt;!--.*?--&gt;|&lt;!DOCTYPE.*?&gt;|\e\$[B@].*?\e\([BDJI]" />

	<css match="@import[^a..z-]" replace="@import-filtered " />
	<css match="behaviour\s*\:" replace="behaviour-filtered:" />
	<css match="-moz-binding\s*\:" replace="-moz-binding-filtered:" />
	<css match="expression\s*\(" replace="expression-filtered(" />
	<css match="url\((?!https?)" replace="url-filtered(" />

	<ignoredata one="&lt;title.*?&gt;.*?&lt;/title.*?&gt;|&lt;script.*?&gt;.*?&lt;/script.*?&gt;|&lt;applet.*?&gt;.*?&lt;/applet.*?&gt;" />

	<lut>
		<any re=".*" />
		<clear re="^all$|^none$|^right$|^left$|" />
		<coords re="[\d,]+" />
		<shape re="circ|circle|poly|polygon|rect|rectangle" />
		<url re="\s*(http|https|ftp|rtsp|gopher)://.*|\s*(callto|h323|h324|mailto|news|cid):.*|/.*|\#.*|\w+\/.*" />
		<size re="-*\d{0,5}(%|px)*" />
		<align re="left|center|right|justify|middle|absmiddle" />
		<valign re="top|middle|bottom|baseline|absmiddle" />
		<method re="get|post" />
		<color re=".{1,20}" />
		<boolean re="true|false|yes|no" />
		<alphanumeric re="[\w\-]+" />
		<stylesheettype re="text\/css" />
		<httpequiv re="Content-Type" />
		<dir re="ltr|rtl" />
	</lut>
	<allowed>
		<legend dir="dir" align="align" class="alphanumeric" contenteditable="any" disabled="any" id="alphanumeric" style="any" title="any" unselectable="any" />
		<map dir="dir" class="alphanumeric" id="alphanumeric" name="any" style="any" title="any" />
