<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.runtime.remoting>
    <application>

      <client>        
        <wellknown type="MArc.Ada.Services.AdaExchange, MArc.Ada.Services" url="tcp://localhost:8017/Core/Exchange"/>
        <wellknown type="MArc.DS.DSTests, MArc.DataSources.Factory" url="tcp://localhost:8017/Core/DSTests"/>        
        <wellknown type="MArc.ExchAuto.ExchangeAutomationRemote, MArc.ExchAuto.Remote" url="tcp://localhost:8017/Core/ExchangeAutomationRemote"/>
        <wellknown type="MArc.Ada.Security.AdaAuthentication, MArc.Ada.Authentication" url="tcp://localhost:8017/Core/ADA"/>
        <wellknown type="MArc.Ada.Services.AdaInformation, MArc.Ada.Services" url="tcp://localhost:8017/Core/Information"/>
        <wellknown type="MArc.UMPolling.Remote.UMRemote, MArc.UMPolling.Remote" url="tcp://localhost:8017/Core/UMPolling"/>
        <wellknown type="ML.NS.NotificationServices, ML.NS" url="tcp://localhost:8017/Core/Notifications"/>        
        <wellknown type="MArc.DataUpload.EmailUploader, MArc.DataUpload" url="tcp://localhost:8017/Core/EmailUploader"/>
      </client>

      <channels>
        <channel ref="tcp" secure="true" tokenImpersonationLevel="Delegation">
          <clientProviders>
            <provider type="MArc.Ada.Security.AdaClientSinkProvider, MArc.Ada.Authentication" />
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
        </channel>
      </channels>

    </application>
    <customErrors mode="off"/>
  </system.runtime.remoting>
</configuration>