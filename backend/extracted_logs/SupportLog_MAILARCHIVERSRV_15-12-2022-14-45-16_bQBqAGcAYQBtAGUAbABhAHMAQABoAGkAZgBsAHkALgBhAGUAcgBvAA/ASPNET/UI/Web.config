      <authorization>
        <allow users="?" />
      </authorization>
    </system.web>
  </location>
  <location path="initializetest.ashx">
    <system.web>
      <authorization>
        <allow users="?" />
      </authorization>
    </system.web>
  </location>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules>
      <remove name="ServiceModel" />
      <add name="Loader" type="MArc.Web.LoaderModule,MArc.Web.Loader" preCondition="managedHandler" />
    </modules>
    <handlers accessPolicy="Read, Script">
      <remove name="ChartImage_axd" />
      <remove name="Telerik_Web_UI_SpellCheckHandler_axd" />
      <remove name="Telerik_Web_UI_DialogHandler_aspx" />
      <remove name="Telerik_RadUploadProgressHandler_ashx" />
      <remove name="Telerik_Web_UI_WebResource_axd" />
      <add name="searchtest.ashx_*" path="searchtest.ashx" verb="*" type="MArc.Web.SearchTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="cachetest.ashx_*" path="cachetest.ashx" verb="*" type="MArc.Web.CacheTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="storetest.ashx_*" path="storetest.ashx" verb="*" type="MArc.Web.DalTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="settingstest.ashx_*" path="settingstest.ashx" verb="*" type="MArc.Web.SettingsTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="securitytest.ashx_*" path="securitytest.ashx" verb="*" type="MArc.Web.SecurityTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="remotingtest.ashx_*" path="remotingtest.ashx" verb="*" type="MArc.Web.RemotingTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="aspnettest.ashx_*" path="aspnettest.ashx" verb="*" type="MArc.Web.AspNetTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="dstest.ashx_*" path="dstest.ashx" verb="*" type="MArc.Web.DSTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="authenticatetest.ashx_*" path="authenticatetest.ashx" verb="*" type="MArc.Web.AuthenticateTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="initializetest.ashx_*" path="initializetest.ashx" verb="*" type="MArc.Web.InitializeTest,MArc.Web.Loader" preCondition="integratedMode,runtimeVersionv4.0" />
      <!-- ASPNET AJAX TreeView Referesh -->
      <add name="ScriptResource" path="ScriptResource.axd" type="System.Web.Handlers.ScriptResourceHandler, System.Web.Extensions" verb="*" preCondition="integratedMode" />
      <add name="Telerik_Web_UI_WebResource_axd" path="Telerik.Web.UI.WebResource.axd" type="Telerik.Web.UI.WebResource" verb="*" preCondition="integratedMode" />
      <add name="ChartImage_axd" path="ChartImage.axd" type="Telerik.Web.UI.ChartHttpHandler" verb="*" preCondition="integratedMode" />
      <add name="Telerik_Web_UI_SpellCheckHandler_axd" path="Telerik.Web.UI.SpellCheckHandler.axd" type="Telerik.Web.UI.SpellCheckHandler" verb="*" preCondition="integratedMode" />
      <add name="Telerik_Web_UI_DialogHandler_aspx" path="Telerik.Web.UI.DialogHandler.aspx" type="Telerik.Web.UI.DialogHandler" verb="*" preCondition="integratedMode" />
      <add name="Telerik_RadUploadProgressHandler_ashx" path="Telerik.RadUploadProgressHandler.ashx" type="Telerik.Web.UI.RadUploadProgressHandler" verb="*" preCondition="integratedMode" />
      <!-- end ASPNET AJAX TreeView Referesh -->
    </handlers>
    <!-- 
      REF: TT651-MA7.1
      This is required so that on IIS7 when HTTP status code 420 which only MAR knows how to handle is sent is not stopped with custom error message from IIS (on Remote) 
      as the ASHX will know how to handle it and respond to it PassThrough=Leaves the response untouched if an existing response exists 
    -->
    <httpErrors existingResponse="PassThrough" />
    <security>
      <authentication>
        <windowsAuthentication enabled="true" />
      </authentication>
    </security>
    <directoryBrowse enabled="false" />
    <defaultDocument>
      <files>
        <clear />
        <add value="Default.htm" />
        <add value="Default.asp" />
        <add value="index.htm" />
        <add value="index.html" />
        <add value="iisstart.htm" />
        <add value="default.aspx" />
        <add value="default.html" />
        <add value="index.asp" />
      </files>
    </defaultDocument>
  </system.webServer>
</configuration>