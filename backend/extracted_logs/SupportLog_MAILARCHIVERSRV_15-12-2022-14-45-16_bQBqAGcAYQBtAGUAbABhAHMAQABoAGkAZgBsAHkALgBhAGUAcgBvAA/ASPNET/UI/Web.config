    <add key="OutlookIntegration.InboxTagAccepted" value="FOLDER_INBOX" />
    <add key="OutlookIntegration.SentTagAccepted" value="FOLDER_SENT" />
    <add key="OutlookIntegration.DelTagAccepted" value="FOLDER_DELETED" />
    <add key="OutlookIntegration.CalendarTagAccepted" value="FOLDER_CALENDAR" />
    <add key="OutlookIntegration.FileTagAccepted" value="FOLDER_FILE" />
    <add key="OutlookIntegration.FileDeletedTagAccepted" value="FOLDER_FILE_DELETED" />
    <add key="OutlookConnectorPath32" value="~/Client Tools/MAROC_32bitOutlook.exe" />
    <add key="OutlookConnectorPath64" value="~/Client Tools/MAROC_64bitOutlook.exe" />
    <add key="ManualArchivingPath" value="~/Client Tools/MAR_ArchiveAssistant.exe" />
    <add key="FileArchiveAssistantX86Path" value="~/Client Tools/MAR_FileArchiveAssistant_32bit.exe" />
    <add key="FileArchiveAssistantX64Path" value="~/Client Tools/MAR_FileArchiveAssistant_64bit.exe" />
    <add key="MailboxCacheMinutesDuration" value="2" />
