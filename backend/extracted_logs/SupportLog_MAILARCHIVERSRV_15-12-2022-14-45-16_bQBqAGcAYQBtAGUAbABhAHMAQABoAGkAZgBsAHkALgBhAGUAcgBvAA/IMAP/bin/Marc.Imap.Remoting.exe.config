<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings file="..\Data\product.config">
  </appSettings>
  <system.diagnostics>
    <switches>
      <add name="Global" value="3" />
    </switches>
    <trace autoflush="true" indentsize="0">
      <listeners>
        <remove name="Default"/>
        <add name="TextListener" type="GFI.Common.DirectoryListener, GFI.Common.Tracing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=4637077a7a2c17fb" initializeData="DebugLogs\"/>

      </listeners>
    </trace>
  </system.diagnostics>
  <system.runtime.remoting>
    <application>
      <channels>       
        <channel ref="ipc" portName="MARImap" secure="true">
          <clientProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>		
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
    <customErrors mode="Off" />
  </system.runtime.remoting>
</configuration>