<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings file="..\Data\product.config">
  </appSettings>
  <runtime>
    <gcServer enabled="true" />
    <gcConcurrent enabled="false" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="ML.Remoting.Server" culture="neutral" />
        <codeBase version="2.0.0.0" href="MArc.MAIS.Remoting.exe" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="ML.Svc.Attendant" culture="neutral" />
        <codeBase version="2.0.0.0" href="MArc.MAIS.exe" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" />
  </startup>
  <system.diagnostics>
    <switches>
      <add name="Global" value="3" />
    </switches>
    <trace autoflush="true" indentsize="0">
      <listeners>
        <remove name="Default" />
        <add name="TextListener" type="GFI.Common.DirectoryListener, GFI.Common.Tracing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=4637077a7a2c17fb" initializeData="DebugLogs\" />
      </listeners>
    </trace>
  </system.diagnostics>
  <system.runtime.remoting>
    <application>
      <client url="ipc://MARMAIS">
        <wellknown type="MArc.Ada.Security.AdaAuthentication, MArc.Ada.Authentication" url="ipc://MARMAIS/MAIS/ADA" />
        <wellknown type="MArc.Ada.Services.AdaInformation, MArc.Ada.Services" url="ipc://MARMAIS/MAIS/Information" />
        <wellknown type="MAIS.Supervisor.Lock, MAIS.Supervisor" url="ipc://MARMAIS/MAIS/Lock" />
        <wellknown type="DLE.Agent, DLE.Agent" url="tcp://localhost:8018/Store/DLEAgent" />
        <wellknown type="GFI.VSS.VssWriterSubscriber, GFI.VSS" url="ipc://MARMAIS/MAIS/VssWriterSubscriber" />
        <wellknown type="ML.NS.NotificationServices, ML.NS" url="tcp://localhost:8017/Core/Notifications" />
      </client>
      <channels>
        <channel ref="tcp" secure="true">
          <clientProviders>
            <provider type="MArc.Ada.Security.AdaClientSinkProvider, MArc.Ada.Authentication" />
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
            <provider type="MArc.Ada.Security.AdaServerSinkProvider, MArc.Ada.Authentication" />
          </serverProviders>
        </channel>
        <channel ref="ipc" secure="true">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
          <clientProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
        </channel>
      </channels>
    </application>
    <customErrors mode="off" />
  </system.runtime.remoting>
</configuration>