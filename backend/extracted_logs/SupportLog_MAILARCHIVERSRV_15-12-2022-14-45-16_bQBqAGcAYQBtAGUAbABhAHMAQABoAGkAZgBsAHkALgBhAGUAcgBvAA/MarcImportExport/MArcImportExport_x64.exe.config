<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="MArcImportExport.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
    </sectionGroup>
  </configSections>

  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>

  <system.diagnostics>
    <switches>
      <add name="Global" value="3"/>
    </switches>
    <trace autoflush="true" indentsize="0">
      <listeners>
        <!--remove name="Default"/-->
		<add name="TextListener" type="GFI.Common.DirectoryListener, GFI.Common.Tracing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=4637077a7a2c17fb" initializeData="DebugLogs\"/>
      </listeners>
    </trace>
  </system.diagnostics>
  <applicationSettings>    
    <MArcImportExport.Properties.Settings>
      <setting name="MArcImportExport_localhost_Search" serializeAs="String">
        <value>http://localhost/Archiver/services/search.asmx</value>
      </setting>
      <setting name="MArcImportExport_MarcSend_Send" serializeAs="String">
        <value>http://localhost/Archiver/services/send.asmx</value>
      </setting>
      <setting name="MArcImportExport_MarcStore_Store" serializeAs="String">
        <value>http://localhost/Archiver/services/store.asmx</value>
      </setting>
      <setting name="FilesInFolder" serializeAs="String">
        <value>5000</value>
      </setting>
      <setting name="MaxFailedAttempts" serializeAs="String">
        <value>10</value>
      </setting>
      <setting name="SearchServiceTimeout" serializeAs="String">
        <value>15</value>
      </setting>
      <setting name="Product" serializeAs="String">
        <value>MAR15</value>
      </setting>
      <setting name="OnlineHelpURL" serializeAs="String">
        <value>http://manuals.gfi.com/{language}/mar15admin/{section}</value>
      </setting>
    </MArcImportExport.Properties.Settings>
  </applicationSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="MArc.Ada.DirectoryEngine" publicKeyToken="F28AA2EFF54BAC51" culture="neutral"/>
        <bindingRedirect oldVersion="0.0.0.0-2.0.0.2" newVersion="2.0.0.2"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>

  <system.runtime.remoting>
    <application>
      <client>
        <wellknown type="MArc.Ada.Security.AdaAuthentication, MArc.Ada.Authentication" url="tcp://localhost:8017/Core/ADA"/>
        <wellknown type="MArc.Ada.Services.AdaInformation, MArc.Ada.Services" url="tcp://localhost:8017/Core/Information"/>
      </client>
      <channels>
        <channel ref="tcp" secure="true">
          <clientProviders>
            <provider type="MArc.Ada.Security.AdaClientSinkProvider, MArc.Ada.Authentication"/>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
            <provider type="MArc.Ada.Security.AdaServerSinkProvider, MArc.Ada.Authentication"/>
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
  
  
  <appSettings>
    <add key="RegKeyURL" value="GFI\MAR15" />
    <add key="EWSMaxBurstTime" value="60000" />
    <add key="EWSRestPeriod" value="10000" />
    <add key="EWSCallThresold" value="1000" />
  </appSettings>
</configuration>
