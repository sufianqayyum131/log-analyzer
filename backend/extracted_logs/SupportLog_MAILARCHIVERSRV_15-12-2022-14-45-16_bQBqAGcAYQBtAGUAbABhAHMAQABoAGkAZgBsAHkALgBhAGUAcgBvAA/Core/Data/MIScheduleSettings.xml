<?xml version="1.0"?>
<ScheduleSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<ImmediateInterval>1</ImmediateInterval>
	<ScheduledInterval>5</ScheduledInterval>
	<TryLaterMaxTimes>6</TryLaterMaxTimes>
	<ArchiveReportEmail>false</ArchiveReportEmail>
	<MaxThreads>3</MaxThreads>
	<UseDynamicThreadCount>true</UseDynamicThreadCount>
	<ReportPlugins>
		<ReportPlugin ReportName="Communications">
			<AssemblyName>bin\MArc.MailInsights.Rep.Conn.dll</AssemblyName>
			<ObjectName>MArc.MailInsights.Rep.ConnectivityGraph</ObjectName>
		</ReportPlugin>
		<ReportPlugin ReportName="EmailResponsiveness">
			<AssemblyName>bin\MArc.MailInsights.Rep.EmailRes.dll</AssemblyName>
			<ObjectName>MArc.MailInsights.Rep.EmailResponsiveness</ObjectName>
		</ReportPlugin>
		<ReportPlugin ReportName="Storage">
			<AssemblyName>bin\MArc.MailInsights.Rep.Storage.dll</AssemblyName>
			<ObjectName>MArc.MailInsights.Rep.EmailStorage</ObjectName>
		</ReportPlugin>
		<ReportPlugin ReportName="WebMail">
			<AssemblyName>bin\MArc.MailInsights.Rep.WebMail.dll</AssemblyName>
			<ObjectName>MArc.MailInsights.Rep.WebmailUsage</ObjectName>
		</ReportPlugin>
		<ReportPlugin ReportName="InappropriateWords">
			<AssemblyName>bin\MArc.MailInsights.Rep.InappWords.dll</AssemblyName>
			<ObjectName>MArc.MailInsights.Rep.InappropriateWords</ObjectName>
		</ReportPlugin>
