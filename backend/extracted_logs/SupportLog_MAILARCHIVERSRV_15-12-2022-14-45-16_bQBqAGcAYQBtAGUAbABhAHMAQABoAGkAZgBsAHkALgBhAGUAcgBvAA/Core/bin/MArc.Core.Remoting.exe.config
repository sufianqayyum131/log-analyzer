<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings file="..\Data\product.config">
	</appSettings>
	<system.diagnostics>
		<switches>
			<add name="Global" value="3" />
		</switches>
		<trace autoflush="true" indentsize="0">
			<listeners>
				<add name="TextListener" type="GFI.Common.DirectoryListener, GFI.Common.Tracing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=4637077a7a2c17fb" initializeData="DebugLogs\"/>
				<remove name="Default"/>
			</listeners>
		</trace>
	</system.diagnostics>
	<system.runtime.remoting>
		<application>
			<client url="ipc://MAR">
			</client>
			<channels>
				<channel ref="tcp" port="8017" secure="true" exclusiveAddressUse="false">
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
						<provider type="MArc.Ada.Security.AdaServerSinkProvider, MArc.Ada.Authentication" />
					</serverProviders>
					<clientProviders>
						<provider type="MArc.Ada.Security.AdaClientSinkProvider, MArc.Ada.Authentication" />
						<formatter ref="binary" typeFilterLevel="Full" />
					</clientProviders>
				</channel>
				<channel ref="ipc" portName="MAR" secure="true" authorizedGroup="System">
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
						<provider type="MArc.Ada.Security.AdaServerSinkProvider, MArc.Ada.Authentication" />
					</serverProviders>
				</channel>
			</channels>
		</application>
		<customErrors mode="Off" />
	</system.runtime.remoting>
</configuration>