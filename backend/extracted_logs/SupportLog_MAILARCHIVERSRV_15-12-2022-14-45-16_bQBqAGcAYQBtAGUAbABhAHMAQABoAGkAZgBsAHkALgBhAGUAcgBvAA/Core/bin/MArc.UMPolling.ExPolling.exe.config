<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings file="..\Data\product.config">
  </appSettings>
  <runtime>
    <gcServer enabled="true" />
    <gcConcurrent enabled="false" />
  </runtime>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" />
  </startup>
  <system.diagnostics>
    <switches>
      <add name="Global" value="3" />
    </switches>
    <trace autoflush="true" indentsize="0">
      <listeners>
        <remove name="Default" />
        <add name="TextListener" type="GFI.Common.DirectoryListener, GFI.Common.Tracing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=4637077a7a2c17fb" initializeData="DebugLogs\" />
      </listeners>
    </trace>
  </system.diagnostics>
  <system.runtime.remoting>
    <application>
      <client url="tcp://localhost:8017">
        <wellknown type="MArc.UMPolling.Remote.UMRemote, MArc.UMPolling.Remote" url="tcp://localhost:8017/Core/UMPolling" />
      </client>
      <channels>
        <channel ref="tcp" secure="true">
          <clientProviders>
            <provider type="MArc.Ada.Security.AdaClientSinkProvider, MArc.Ada.Authentication" />
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
            <provider type="MArc.Ada.Security.AdaServerSinkProvider, MArc.Ada.Authentication" />
          </serverProviders>
        </channel>
      </channels>
    </application>
    <customErrors mode="Off" />
  </system.runtime.remoting>
</configuration>