<?xml version="1.0"?>
<configuration>
	<appSettings file="..\Data\product.config">
	</appSettings>
	
<system.diagnostics>
     <switches>
          <add name="Global" value="3"/>
     </switches>

      <trace autoflush="true" indentsize="0">
         <listeners>
          <add name="TextListener" type="GFI.Common.DirectoryListener, GFI.Common.Tracing, Version=1.0.0.1, Culture=neutral, PublicKeyToken=4637077a7a2c17fb" initializeData=".\DebugLogs"/>
         </listeners>
      </trace>

 </system.diagnostics>

  <system.runtime.remoting>
    	<application>
    	 <client url="tcp://localhost:8014">	
            <wellknown type="MArc.CS.CSCoordinator, MArc.ConfigurationServices" url="tcp://localhost:8014/Core/CS"/>
            <wellknown type="MArc.Ada.Services.AdaExchange, MArc.Ada.Services" url="tcp://localhost:8014/Core/Exchange"/>            
            <wellknown type="MArc.Ada.Services.AdaInformation, MArc.Ada.Services" url="tcp://localhost:8014/Core/Information"/>            
         </client>
         <channels>
            <channel ref="tcp">
            <serverProviders> 
				<formatter ref="soap" typeFilterLevel="Full"/>
				<formatter ref="binary" typeFilterLevel="Full"/>
            </serverProviders>
           <clientProviders>
             <formatter ref="binary"/>
               <provider type="MArc.Ada.Security.AdaClientSinkProvider, MArc.Ada.Authentication"/>
             	            
               <provider type="Microsoft.Samples.Runtime.Remoting.Security.SecurityClientChannelSinkProvider, Microsoft.Samples.Runtime.Remoting.Security, Culture=neutral, PublicKeyToken=9f0928b6ea506bde, Version=2.0.18.0" securityPackage="negotiate" impersonationLevel="impersonate" authenticationLevel="call"/> 
           </clientProviders>

            </channel>
         </channels>
	</application>
	<customErrors mode="off"/>
  </system.runtime.remoting>
	
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
