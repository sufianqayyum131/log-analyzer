<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings file="..\Data\product.config">
  </appSettings>
  <runtime>
    <gcServer enabled="true" />
    <gcConcurrent enabled="false" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="ML.Remoting.Server" culture="neutral" />
        <codeBase version="2.0.0.0" href="MArc.Core.Remoting.exe" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="ML.Svc.Attendant" culture="neutral" />
        <codeBase version="2.0.0.0" href="MArc.Core.exe" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" />
  </startup>
  <system.diagnostics>
    <switches>
      <add name="Global" value="3" />
    </switches>
    <trace autoflush="true" indentsize="0">
      <listeners>
        <remove name="Default" />
        <add name="TextListener" type="GFI.Common.DirectoryListener, GFI.Common.Tracing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=4637077a7a2c17fb" initializeData="DebugLogs\" />
      </listeners>
    </trace>
  </system.diagnostics>
  <system.runtime.remoting>
    <application>
      <client url="ipc://MAR">
        <wellknown type="MArc.Ada.Services.AdaExchange, MArc.Ada.Services" url="ipc://MAR/Core/Exchange" />
        <wellknown type="MArc.Ada.Security.AdaAuthentication, MArc.Ada.Authentication" url="ipc://MAR/Core/ADA" />
        <wellknown type="MArc.Ada.Services.AdaInformation, MArc.Ada.Services" url="ipc://MAR/Core/Information" />
        <wellknown type="DLE.Agent, DLE.Agent" url="tcp://localhost:8018/Store/DLEAgent" />
        <wellknown type="GFI.VSS.VssWriterSubscriber, GFI.VSS" url="ipc://MAR/Core/VssWriterSubscriber" />
        <wellknown type="MArc.UMPolling.Remote.UMRemote, MArc.UMPolling.Remote" url="ipc://MAR/Core/UMPolling" />
        <wellknown type="MArc.IMAP.Remote.IMAPRemote, MArc.IMAP.Remote" url="tcp://localhost:8022/Imap/ImapRemote" />
        <wellknown type="Store.Dal.IMailInsights, Store.Dal" url="tcp://localhost:8018/Store/MailInsightsReporting" />
        <wellknown type="Store.Cache.Statistics, Store.Cache" url="tcp://localhost:8018/Store/Statistics" />
        <wellknown type="ML.NS.NotificationServices, ML.NS" url="ipc://MAR/Core/Notifications" />
        <wellknown type="MArc.DS.DSTests, MArc.DataSources.Factory" url="ipc://MAR/Core/DSTests" />
      </client>
      <channels>
        <channel ref="tcp" secure="true">
          <clientProviders>
            <provider type="MArc.Ada.Security.AdaClientSinkProvider, MArc.Ada.Authentication" />
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
            <provider type="MArc.Ada.Security.AdaServerSinkProvider, MArc.Ada.Authentication" />
          </serverProviders>
        </channel>
        <channel ref="ipc" secure="true">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
          <clientProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
        </channel>
      </channels>
    </application>
    <customErrors mode="Off" />
  </system.runtime.remoting>
</configuration>