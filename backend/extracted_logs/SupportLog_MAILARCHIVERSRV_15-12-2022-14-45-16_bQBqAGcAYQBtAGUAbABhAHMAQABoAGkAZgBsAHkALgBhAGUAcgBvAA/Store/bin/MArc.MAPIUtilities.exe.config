<?xml version="1.0"?>
<configuration>
  <system.diagnostics>
    <switches>
      <add name="Global" value="3" />
    </switches>
    <trace autoflush="true" indentsize="0">
      <listeners>
        <remove name="Default" />
        <add name="TextListener" type="GFI.Common.DirectoryListener, GFI.Common.Tracing, Version=2.0.0.0, Culture=neutral, PublicKeyToken=4637077a7a2c17fb" initializeData="DebugLogs\" />
      </listeners>
    </trace>
  </system.diagnostics>
  <system.runtime.remoting>
    <application>
      <service>
        <wellknown mode="Singleton" type="MArc.MAPIUtilities.MAPIUtilities, MArc.MAPIUtilities" objectUri="MAPIUtilities" />
      </service>
      <channels>
        <channel ref="ipc" portName="MAPIUtilities" secure="true">
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>