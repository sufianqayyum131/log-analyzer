<?xml version="1.0"?>
<configuration>
	<system.diagnostics>
		<switches>
			<add name="Global" value="3" />
		</switches>
		<trace autoflush="true" indentsize="0">
			<listeners>
				<remove name="Default" />
			</listeners>
		</trace>
	</system.diagnostics>
	<system.runtime.remoting>
		<application>
			<service>
				<wellknown mode="Singleton" type="MArc.MAPIUtilities.MAPIUtilities, MArc.MAPIUtilities_x64" objectUri="MAPIUtilities" />
			</service>
			<channels>
				<channel ref="ipc" portName="MAPIUtilities" secure="true">
					<serverProviders>
						<formatter ref="binary" typeFilterLevel="Full" />
					</serverProviders>
				</channel>
			</channels>
		</application>
	</system.runtime.remoting>
</configuration>